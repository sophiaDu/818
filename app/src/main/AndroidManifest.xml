<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teboz.egg"
    android:versionCode="2"
    android:versionName="1.0">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.RAISED_THREAD_PRIORITY" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL" />


    <uses-permission android:name="android.permission.WAKE_LOCK"/>


    <application
        android:name=".activity.MyApplication"
        android:icon="@mipmap/egg_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SelectRoleActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.AddRoleActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.RecorderActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.RecorderPowerActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.ListenActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.AboutActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.PrivacyActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.TicklingActivity" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize"></activity>
        <activity android:name=".activity.VipActivity" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize|adjustPan"></activity>
        <activity android:name=".activity.DIYActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.HighTogetherActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.SetPowerSoundActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".activity.SaveNameActivity"
            android:windowSoftInputMode="stateVisible|adjustPan"  android:screenOrientation="portrait"></activity>
      <!--  <activity
            android:name=".activity.DemandSubActivity"
            android:screenOrientation="portrait"></activity>-->
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
